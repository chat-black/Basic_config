" This is my configure file
:set background=dark
:set nocompatible

:filetype on
:filetype indent on
:syntax on
:set t_Co=256

:set number relativenumber
:set showcmd ruler
:set showmatch matchtime=1
:set showmode

:set expandtab
:set shiftwidth=4
:set tabstop=4
:set smarttab
:set softtabstop=4
:set shiftround

:set scrolloff=3
:set backspace=indent,eol,start
:set nobackup
:set hlsearch
:set incsearch


:set ignorecase
:set smartcase

"##############################


:abbreviate #i #include
:abbreviate #d #define
